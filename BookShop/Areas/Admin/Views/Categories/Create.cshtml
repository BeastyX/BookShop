@model Category@{
    ViewData["Title"] = "Nova kategorije";
}<form asp-action="Create" method="post">    @Html.AntiForgeryToken()    <div class="form-floating">        <input asp-for="Name" class="form-control shadow border-0" placeholder=""/>        <label asp-for="Name"></label>        <span asp-validation-for="Name" class="text-danger"></span>    </div>    <div class="mt-5">        <button class="btn btn-primary" type="submit">Kreiraj</button>    </div></form>@section Scripts{    <partial name="_ValidationScriptsPartial" />}